<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #1E90FF;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controles.ShowForm">
   <children>
      <AnchorPane layoutX="300.0" layoutY="-2.0" prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: #ffffff;">
         <children>
            <TableView fx:id="showForm" layoutX="25.0" layoutY="86.0" prefHeight="200.0" prefWidth="227.0">
               <columns>
                  <TableColumn fx:id="id" editable="false" prefWidth="75.0" text="id" />
                  <TableColumn fx:id="date" prefWidth="75.0" text="date" />
                  <TableColumn fx:id="statut" prefWidth="75.0" text="statut" />
               </columns>
            </TableView>
            <Label layoutX="50.0" layoutY="40.0" text="liste des formulaires" textAlignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="donnees" layoutX="175.0" layoutY="322.0" mnemonicParsing="false" onAction="#afficherDonnees" text="voir les donnees" />
            <Button fx:id="form" layoutX="74.0" layoutY="360.0" mnemonicParsing="false" onAction="#search" text="recherche sur specifique formulaire" />
         </children>
      </AnchorPane>
      <Label layoutX="22.0" layoutY="107.0" text="id" textFill="WHITE" />
      <Label layoutX="15.0" layoutY="153.0" text="date" textFill="WHITE" />
      <Label layoutX="12.0" layoutY="200.0" text="statut" textFill="WHITE" />
      <TextField fx:id="idTextField" editable="false" layoutX="54.0" layoutY="103.0" />
      <TextField fx:id="dateTextField" editable="false" layoutX="54.0" layoutY="149.0" />
      <TextField fx:id="statutTextField" layoutX="54.0" layoutY="196.0" />
      <Button layoutX="32.0" layoutY="249.0" mnemonicParsing="false" onAction="#modifierFormulaire" style="-fx-background-color: #ffffff;" text="modifier" />
      <Button layoutX="160.0" layoutY="249.0" mnemonicParsing="false" onAction="#supprimerFormulaire" style="-fx-background-color: #ffffff;" text="supprimer" />
      <Button layoutX="48.0" layoutY="349.0" mnemonicParsing="false" onAction="#trierFormulaire" text="trier formulaire selon statut" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controles.AfficherFormulaire">
   <children>
      <TableView fx:id="showFormulaire" layoutX="300.0" layoutY="100.0" prefHeight="200.0" prefWidth="227.0">
        <columns>
          <TableColumn fx:id="id" editable="false" prefWidth="75.0" text="id" />
          <TableColumn fx:id="date" prefWidth="75.0" text="date" />
            <TableColumn fx:id="statut" prefWidth="75.0" text="statut" />
        </columns>
      </TableView>
      <Label layoutX="325.0" layoutY="55.0" text="liste des formulaires" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="100.0" text="id" />
      <Label layoutX="14.0" layoutY="144.0" text="date" />
      <Label layoutX="14.0" layoutY="191.0" text="statut" />
      <TextField fx:id="idTextField" editable="false" layoutX="58.0" layoutY="96.0" />
      <TextField fx:id="dateTextField" layoutX="59.0" layoutY="140.0" />
      <TextField fx:id="statutTextField" layoutX="59.0" layoutY="187.0" />
      <Button layoutX="31.0" layoutY="242.0" mnemonicParsing="false" onAction="#modifierFormulaire" text="update" />
      <Button layoutX="158.0" layoutY="242.0" mnemonicParsing="false" onAction="#supprimerFormulaire" text="delete" />
      <Button fx:id="donnees" layoutX="476.0" layoutY="341.0" mnemonicParsing="false" onAction="#afficherDonnees" text="voir les donnees" />
   </children>
</AnchorPane>

      <Button layoutX="416.0" layoutY="34.0" mnemonicParsing="false" onAction="#searchForm" text="search" />
      <TableView fx:id="searchForm" layoutX="186.0" layoutY="100.0" prefHeight="200.0" prefWidth="227.0">
         <columns>
            <TableColumn fx:id="id" editable="false" prefWidth="75.0" text="id" />
            <TableColumn fx:id="date" prefWidth="75.0" text="date" />
            <TableColumn fx:id="statut" prefWidth="75.0" text="statut" />
         </columns>
      </TableView>
      <Button fx:id="formulaire" layoutX="14.0" layoutY="345.0" mnemonicParsing="false" onAction="#afficherFormulaire" text="revenir a la page principal" />
   </children>
</AnchorPane>
